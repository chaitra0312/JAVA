import java.lang.Exception;
class Nandu {
public static void main(String[]arg) {
try {
int a=10;
int b;
for(b=10;b>=0;b--) {
System.out.println(a/b);
}
}
catch(ArithmeticException e)
{
System.out.println(e.toString());
}
}
}

//WITHOUT USING LANG AND EXCEPTION CLASS
import java.util.*;
class Nandu { 
public static void main(String[]arg) {
int a=10;
int b;
for(b=10;b>=0;b--) {
System.out.println(a/b);
}
}
}


