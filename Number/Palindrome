import java.util.*;
class Palindrome
{
	public static void main(String[]arg)
		{
		int rem, temp, sum=0;
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		temp=n;
		while(n>0) {
		rem=n%10;
		sum=sum*10+rem;
		n=n/10;
		}
		if(sum==temp) {
		System.out.println("It is a palindrome number");
		} else {
		System.out.println("It is Not a palindrome number");
		}
	}
}		
