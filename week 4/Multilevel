import java.util.*;
class A {
public void methodA() {
System.out.println("A is a method");
}
}
class B extends A {
public void methodB() {
System.out.println("B is a method");
}
}
class C extends B {
public void methodC() {
System.out.println("C is a method");
}
public static void main(String[]arg) {
C obj=new C();
obj.methodA();
obj.methodB();
obj.methodC();
}
}
